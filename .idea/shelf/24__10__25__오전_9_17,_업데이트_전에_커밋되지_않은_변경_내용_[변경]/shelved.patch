+
+
+public class Board_Insert {
+    public static void main(String[] args) {
+        Connection conn = null;
+        try {
+            // JDBC Driver 를 등록
+            Class.forName("oracle.jdbc.OracleDriver");
+
+            // 연결
+            conn = DriverManager.getConnection(
+                    "jdbc:oracle:thin:@localhost:1521:xe",
+                    "system",
